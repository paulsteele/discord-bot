{
  "defaultSeverity": "error",
  "extends": ["tslint-config-airbnb", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "variable-name": false,
    "function-name": false,
    "semicolon": { "severity": "error" },
    "ordered-imports": true
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": [
      "node_modules",
      "dist"
    ]
  }
}
