{
  "name": "teyler_bot",
  "version": "2.0.0",
  "description": "A discord bot inspired by teyler!",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e tokens.env ts-node-dev src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "lint": "tslint -c ./tslint.json './**/*.ts'",
    "test": "jest .",
    "install-ffmpeg": "ffbinaries ffmpeg -o ./node_modules/.bin",
    "test:watch": "jest --watch .",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulsteele/discord-bot.git"
  },
  "author": "Paul Steele",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paulsteele/discord-bot/issues"
  },
  "homepage": "https://github.com/paulsteele/discord-bot#readme",
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@types/node": "^12.6.8",
    "dotenv-cli": "^2.0.1",
    "jest": "^24.8.0",
    "nodemon": "^1.11.0",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.18.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "@discordjs/uws": "^11.149.1",
    "bufferutil": "^4.0.1",
    "discord.js": "^11.5.1",
    "erlpack": "github:hammerandchisel/erlpack",
    "ffbinaries": "^1.0.3",
    "libsodium": "^0.7.5",
    "opusscript": "0.0.6",
    "ytdl-core": "^0.29.2"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/*test.js",
      "!**/coverage/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/coverage/"
    ]
  }
}
