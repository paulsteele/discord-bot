{
  "name": "teyler_bot",
  "version": "2.3.0",
  "description": "A discord bot inspired by teyler!",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "build": "tsc",
    "serve": "node dist/index.js",
    "lint": "tslint -c ./tslint.json 'src/**/*.ts'",
    "test": "jest .",
    "install-ffmpeg": "ffbinaries ffmpeg -o ./node_modules/.bin",
    "test:watch": "jest --watch .",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulsteele/discord-bot.git"
  },
  "author": "Paul Steele",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/paulsteele/discord-bot/issues"
  },
  "homepage": "https://github.com/paulsteele/discord-bot#readme",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^26.0.3",
    "@types/node": "^13.1.4",
    "dotenv-cli": "^3.1.0",
    "jest": "^24.9.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.18.2",
    "regenerator-runtime": "^0.13.6",
    "ts-jest": "^24.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "bufferutil": "^4.0.1",
    "discord.js": "^11.5.1",
    "express": "^4.17.1",
    "ffbinaries": "^1.1.4",
    "libsodium": "^0.7.5",
    "node-opus": "^0.3.2",
    "ytdl-core": "^4.5.0"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.js",
      "!**/node_modules/**",
      "!**/dist/**",
      "!**/*test.ts",
      "!**/coverage/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "preset": "ts-jest",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/coverage/"
    ]
  }
}
